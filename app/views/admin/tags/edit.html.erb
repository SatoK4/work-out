<%= flash[:notice] %>
<%= flash[:alert] %>

<h3>タグ編集</h3>

<%= form_with model: @tag, url: admin_tag_path, method: :patch do |f| %>
  <table>
    <tr>
      <td>
        <label>タグ名</label>
      </td>
      <td><%= f.text_field :name %></td>
      <td><%= f.submit "変更を保存" %></td>
    </tr>
  </table>
<% end %>