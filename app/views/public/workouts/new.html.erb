<h3>トレーニング新規登録</h3>

<%= form_with model: @workout, url: workouts_path, method: :post do |f| %>
  <table>
    <tr>
      <td><%= f.label :"イメージ画像" %></td>
      <td><%= f.file_field :image, accept: "image/*" %></td>
    </tr>
    <tr>
      <td><%= f.label :"トレーニング名" %></td>
      <td><%= f.text_field :name %></td>
    </tr>
    <tr>
      <td><%= f.label :"トレーニング内容" %></td>
      <td><%= f.text_area :introduction %></td>
    </tr>
  </table>
  <%= f.submit '投稿' %>
<% end %>