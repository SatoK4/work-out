<%= flash[:alert] %>
<h3>トレーニング新規登録</h3>

<%= form_with model: @workout do |f| %>
  <table>
    <tr>
      <td><%= f.label :"イメージ画像" %></td><!--プレビューを表示-->
      <td><%= f.file_field :images, multiple: true %></td>
    </tr>
    <tr>
      <td><%= f.label :"トレーニング名" %></td>
      <td><%= f.text_field :name %></td>
    </tr>
    <tr>
      <td><%= f.label :"トレーニング内容" %></td>
      <td><%= f.text_area :introduction %></td>
    </tr>
  </table>
  <%= f.submit '投稿' %>
<% end %>